   "source": [
    "def equations(p):\n",
    "    x, y = p\n",
    "    return (x**2 - y, np.exp(x) - y**2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "98554a06-c296-4c97-a999-2f106b28a85b",
   "metadata": {
    "tags": []
   },
   "outputs": [],
   "source": [
    "sol=fsolve(equations,(10,10))"
   ]
  },
